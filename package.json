    "dist/howler.js",
    "dist/howler.min.js",
    "dist/howler.core.min.js",
    "dist/howler.spatial.min.js",
    "LICENSE.md"
  ],
  "dependencies": {
    "fs-extra": "^10.0.0"
  }
